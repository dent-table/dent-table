    "@typescript-eslint/eslint-plugin": "4.26.1",
    "@typescript-eslint/eslint-plugin-tslint": "4.26.1",
    "@typescript-eslint/parser": "4.26.1",
    "chai": "4.3.4",
    "conventional-changelog-cli": "2.1.1",
    "core-js": "3.6.5",
    "cross-env": "7.0.3",
    "date-fns-tz": "^1.1.4",
