    "@angular/material": "^9.2.4",
    "@angular/material-moment-adapter": "^9.2.4",
    "better-sqlite3": "^7.1.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.21",
    "material-icons": "^0.3.1",
    "mobile-drag-drop": "^2.3.0-rc.1",
    "moment": "^2.29.1",
    "ng-mat-search-bar": "^10.0.0-beta.3",
    "ng2-dnd": "^5.0.2",
    "tslib": "^2.0.0",
    "winston": "^3.3.3"
  },
